			<ANIM_NAME>LANDING_GEAR_#INTERACTION_TYPE#_Check_Down</ANIM_NAME>
			<PART_ID>LANDING_GEAR_#INTERACTION_TYPE#_Check_Down</PART_ID>
			<WWISE_EVENT_1>landing_gear_checkdown_push_button_on</WWISE_EVENT_1>
