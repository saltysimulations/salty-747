				</Case>
				<Case Value="AIRBUS">
					<SET_STATE_EXTERNAL>
						1 -1 p0 0 &gt; ? sp0
						#HEADING_INDEX# (A:AUTOPILOT HEADING LOCK DIR:#HEADING_INDEX#, degrees) 1 l0 * + dnor (&gt;K:2:HEADING_BUG_SET)
						p0 0 &gt; if{ (&gt;H:A320_Neo_CDU_AP_INC_HEADING) } els{ (&gt;H:A320_Neo_CDU_AP_DEC_HEADING) }
