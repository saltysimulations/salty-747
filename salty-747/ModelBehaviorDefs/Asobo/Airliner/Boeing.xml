<Template Name="ASOBO_COM_Template_Boeing">
	<DefaultTemplateParameters>
		<PANEL_NAME>Boeing_Com</PANEL_NAME>
		<COM_INDEX>0</COM_INDEX>
	</DefaultTemplateParameters>
	
	<Component ID="com#COM_INDEX#_led_sel" Node="com#COM_INDEX#_led_sel">
		<UseTemplate Name="ASOBO_GT_Visibility_Code">
			<VISIBILITY_CODE>0</VISIBILITY_CODE>
		</UseTemplate>
	</Component>
	
	<!--VHF MODE SELECTION-->
	<UseTemplate Name="ASOBO_COM_BUTTON_Template_Boeing">
		<TOOLTIPID>TT:COCKPIT.TOOLTIPS.COM_PUSH_VHF_CYCLE</TOOLTIPID>
		<BASE_NAME>vhf</BASE_NAME>
		<RUN_ON_PRESS>
			(L:XMLVAR_COM#COM_INDEX#_CURRENT_FREQ) 0 == if{
				1 (&gt;L:XMLVAR_COM#COM_INDEX#_CURRENT_FREQ)		(* 0 -> 1 *)
			}
			els{
				(L:XMLVAR_COM#COM_INDEX#_CURRENT_FREQ) 1 == if{
					2 (&gt;L:XMLVAR_COM#COM_INDEX#_CURRENT_FREQ)	(* 1 -> 2 *)
				}
				els{
					0 (&gt;L:XMLVAR_COM#COM_INDEX#_CURRENT_FREQ)	(* 2 or anything else -> 0 *)
				}
			}
			
			
		</RUN_ON_PRESS>
	</UseTemplate>
	
	<UseTemplate Name="ASOBO_COM_FREQ_LED_Template_Boeing">
		<LED_NAME>vhf_L</LED_NAME>
		<FREQ_INDEX>0</FREQ_INDEX>
	</UseTemplate>
	
	<UseTemplate Name="ASOBO_COM_FREQ_LED_Template_Boeing">
		<LED_NAME>vhf_C</LED_NAME>
		<FREQ_INDEX>1</FREQ_INDEX>
	</UseTemplate>
	
	<UseTemplate Name="ASOBO_COM_FREQ_LED_Template_Boeing">
		<LED_NAME>vhf_R</LED_NAME>
		<FREQ_INDEX>2</FREQ_INDEX>
	</UseTemplate>
	
	<!--HF MODE SELECTION-->
	<UseTemplate Name="ASOBO_COM_BUTTON_Template_Boeing">
		<TOOLTIPID>TT:COCKPIT.TOOLTIPS.COM_PUSH_HF_CYCLE</TOOLTIPID>
		<BASE_NAME>hf</BASE_NAME>
		<RUN_ON_PRESS>
			(L:XMLVAR_COM#COM_INDEX#_CURRENT_FREQ) 3 == if{
				4 (&gt;L:XMLVAR_COM#COM_INDEX#_CURRENT_FREQ)	(* 3 -> 4 *)
			}
			els{
				3 (&gt;L:XMLVAR_COM#COM_INDEX#_CURRENT_FREQ)	(* 4 or anything else -> 3 *)
			}
