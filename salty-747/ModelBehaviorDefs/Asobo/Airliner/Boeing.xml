<Template Name="ASOBO_BOEING_Knob_StandbyPower_Template">
	<DefaultTemplateParameters>
		<SWITCH_DIRECTION>Horizontal</SWITCH_DIRECTION>
		<ARROW_TYPE>Curved</ARROW_TYPE>
		<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
		<SWITCH_POSITION_VAR>XMLVAR_StandbyPower_Selector</SWITCH_POSITION_VAR>
		<PART_ID>BOEING_StandbyPower_Selector</PART_ID>
		<WWISE_EVENT>standbypower_selector_knob</WWISE_EVENT>
		<ANIMTIP_0>TT:COCKPIT.TOOLTIPS.AVIONICS_STBY_POWER_BUTTON_SET_OFF</ANIMTIP_0>
		<ANIMTIP_1>TT:COCKPIT.TOOLTIPS.AVIONICS_STBY_POWER_BUTTON_SET_AUTO</ANIMTIP_1>
		<ANIMTIP_2>TT:COCKPIT.TOOLTIPS.AVIONICS_STBY_POWER_BUTTON_SET_BAT</ANIMTIP_2>
	</DefaultTemplateParameters>
	
	<Component ID="#NODE_ID#" Node="#NODE_ID#">
		<UseTemplate Name="ASOBO_GT_Switch_3States">
			<CODE_POS_0>
				#BATTERY_BUS_ID# (&gt;A:BUS LOOKUP INDEX, Number) (A:BUS CONNECTION ON:#MAIN_BUS_ID#, Bool) if{
					#BATTERY_BUS_ID# #MAIN_BUS_ID# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE)
				}
			</CODE_POS_0>
			<CODE_POS_1>
				#BATTERY_BUS_ID# (&gt;A:BUS LOOKUP INDEX, Number) (A:BUS CONNECTION ON:#MAIN_BUS_ID#, Bool) ! if{
					#BATTERY_BUS_ID# #MAIN_BUS_ID# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE)
				}
			</CODE_POS_1>
			<STATE0_TEST>#BATTERY_BUS_ID# (&gt;A:BUS LOOKUP INDEX, Number) (A:BUS CONNECTION ON:#MAIN_BUS_ID#, Bool) ! </STATE0_TEST>
			<STATE1_TEST>#BATTERY_BUS_ID# (&gt;A:BUS LOOKUP INDEX, Number) (A:BUS CONNECTION ON:#MAIN_BUS_ID#, Bool) (#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) 2 != and</STATE1_TEST>
		</UseTemplate>
	</Component>
</Template>

