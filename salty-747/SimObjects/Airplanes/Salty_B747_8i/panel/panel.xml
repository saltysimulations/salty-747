	<VoicesAlerts>

		<Alert>
			<Type>SoundOnly</Type>
			<SoundEvent>wailer</SoundEvent>
			<Condition>
				<StateMachine>
					<State id="Off" value="0">
						<Transition to="On">
							<Equal>
								<Simvar name="AUTOPILOT MASTER" unit="Boolean"/>
								<Constant>1</Constant>
