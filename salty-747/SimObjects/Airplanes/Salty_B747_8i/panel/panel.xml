	<!-- Annunciations -->

	<Annunciations>
		<Annunciation>
			<Type>Warning</Type>
			<Text>AUTOPILOT DISC</Text>
			<Condition>
				<StateMachine>
					<State id="Off" value="0">
						<Transition to="On">
							<Equal>
								<Simvar name="AUTOPILOT MASTER" unit="Boolean"/>
								<Constant>1</Constant>
							</Equal>
						</Transition>
					</State>
					<State id="On" value="0">
						<Transition to="Alerting">
							<Equal>
								<Simvar name="AUTOPILOT MASTER" unit="Boolean"/>
								<Constant>0</Constant>
							</Equal>
						</Transition>
					</State>
					<State id="Alerting" value="1">
						<Transition to="On">
							<Equal>
								<Simvar name="AUTOPILOT MASTER" unit="Boolean"/>
								<Constant>1</Constant>
							</Equal>
						</Transition>
					</State>
				</StateMachine>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
