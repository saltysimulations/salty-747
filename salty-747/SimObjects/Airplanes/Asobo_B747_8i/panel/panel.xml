	<!-- Voices Alerts -->
  
	<VoicesAlerts>
		<Alert>
			<Type>SoundOnly</Type>
			<SoundEvent>tone_altitude_alert_default</SoundEvent>
			<Condition>
				<StateMachine>
					<State id="Armed" value="0">
						<Transition to="Alerting">
			  				<Equal>
								<Simvar name="L:SALTY_ALT_ALERT" unit="Boolean"/>
			 					<Constant>1</Constant>
			  				</Equal>
						</Transition>
					</State>
					<State id="Alerting" value="1">
						<Transition to="Disarmed">
			  				<Equal>
								<Simvar name="L:SALTY_ALT_ALERT" unit="Boolean"/>
			 					<Constant>1</Constant>
			  				</Equal>
						</Transition>
					</State>
					<State id="Disarmed" value="0">
						<Transition to="Armed">
			  				<Equal>
								<Simvar name="L:SALTY_ALT_ALERT" unit="Boolean"/>
			 					<Constant>0</Constant>
			  				</Equal>
						</Transition>
					</State>
				</StateMachine>
			</Condition>
		</Alert>

		<Alert>
			<Type>SoundOnly</Type>
			<SoundEvent>aural_minimums</SoundEvent>
			<Condition>
				<StateMachine>
					<State id="Under" value="0">
						<Transition to="Over">
							<GreaterEqual>
								<Simvar name="INDICATED ALTITUDE" unit="feet"/>
								<Add>
									<Simvar name="L:SALTY_MINIMUMS_ALT" unit="feet"/>
									<Constant>55</Constant>
								</Add>
							</GreaterEqual>
						</Transition>
					</State>
					<State id="Over" value="0">
						<Transition to="Alerting">
							<LowerEqual>
								<Simvar name="INDICATED ALTITUDE" unit="feet"/>
								<Simvar name="L:SALTY_MINIMUMS_ALT" unit="feet"/>
							</LowerEqual>
						</Transition>
					</State>
					<State id="Alerting" value="1">
						<Transition to="Under">
							<Lower>
								<Simvar name="INDICATED ALTITUDE" unit="feet"/>
								<Subtract>
									<Simvar name="L:SALTY_MINIMUMS_ALT" unit="feet"/>
									<Constant>1</Constant>
								</Subtract>
							</Lower>
						</Transition>
					</State>
				</StateMachine>
			</Condition>
		</Alert>

		<Alert>
		  <Type>Warning</Type>
		  <SoundEvent>aural_landing_gear</SoundEvent>
		  <ShortText>L/G GEAR NOT DOWN</ShortText>
		  <LongText>L/G GEAR NOT DOWN</LongText>
		  <Condition>
			<StateMachine>
				<State id="Under" value="0">
					<Transition to="Over">
						<GreaterEqual>
							<Simvar name="PLANE ALT ABOVE GROUND" unit="feet"/>
							<Constant>1500</Constant>
						</GreaterEqual>
					</Transition>
				</State>
				<State id="Over" value="0">
					<Transition to="Alerting">
						<And>
							<Lower>
								<Simvar name="PLANE ALT ABOVE GROUND" unit="feet"/>
								<Constant>750</Constant>
							</Lower>
							<Or>
								<Lower>
									<Simvar name="GEAR RIGHT POSITION" unit="Percent"/>
									<Constant>100</Constant>
								</Lower>
								<Lower>
									<Simvar name="GEAR CENTER POSITION" unit="Percent"/>
									<Constant>100</Constant>
								</Lower>
								<Lower>
									<Simvar name="GEAR LEFT POSITION" unit="Percent"/>
									<Constant>100</Constant>
								</Lower>
							</Or>
							<Or>
								<GreaterEqual>
									<Simvar name="FLAPS HANDLE INDEX" unit="number"/>
									<Constant>3</Constant>
								</GreaterEqual>
								<LowerEqual>
									<Simvar name="GENERAL ENG THROTTLE LEVER POSITION:1" unit="Percent"/>
									<Constant>0</Constant>
								</LowerEqual>
							</Or>
						</And>
					</Transition>
				</State>
				<State id="Alerting" value="1">
					<Transition to="Over">
						<Or>
							<GreaterEqual>
								<Simvar name="PLANE ALT ABOVE GROUND" unit="feet"/>
								<Constant>750</Constant>
							</GreaterEqual>
							<And>
								<Equal>
									<Simvar name="GEAR RIGHT POSITION" unit="Percent"/>
									<Constant>100</Constant>
								</Equal>
								<Equal>
									<Simvar name="GEAR CENTER POSITION" unit="Percent"/>
									<Constant>100</Constant>
								</Equal>
								<Equal>
									<Simvar name="GEAR LEFT POSITION" unit="Percent"/>
									<Constant>100</Constant>
								</Equal>
							</And>
						</Or>
					</Transition>
				</State>
			</StateMachine>
		  </Condition>
		</Alert>
		
