		<Annunciation>
		  <Type>Advisory</Type>
		  <Text>\FUEL TANK/ENG</Text>
		  <Condition>
			<And>
				<Or>
					<LowerEqual>
						<Simvar name="FUEL TANK LEFT MAIN QUANTITY" unit="gallon"/>
